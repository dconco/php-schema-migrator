{
	"name": "dconco/schema-migrator",
	"description": "A beautiful command-line database migration tool for any PHP project. Laravel-style migrations without the framework.",
	"type": "library",
	"keywords": ["database", "migration", "cli", "schema", "mysql", "postgresql", "sqlite"],
	"license": "MIT",
	"authors": [
		{
			"name": "dconco",
			"email": "dconco@example.com"
		}
	],
	"require": {
		"php": "^8.1",
		"illuminate/database": "^11.0|^12.0",
		"illuminate/events": "^11.0|^12.0",
		"symfony/console": "^6.0|^7.0",
		"symfony/yaml": "^6.0|^7.0"
	},
	"autoload": {
		"psr-4": {
			"Dconco\\SchemaMigrator\\": "src/"
		}
	},
	"bin": [
		"bin/schema-migrator"
	],
	"minimum-stability": "stable",
	"prefer-stable": true
}
